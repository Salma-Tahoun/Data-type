#include <bits/stdc++.h>
#include <iostream>
using namespace std;
int main (){
   char X ;
   cin >>X ; 
   if (X>='a'&&X<='z'){
       cout <<"ALPHA"<<endl;
       cout <<"IS SMALL";
       }
   if (X>='A'&&X<='Z'){
      cout <<"ALPHA"<<endl;
       cout <<"IS CAPITAL";

   }
  
if (X>='0'&&X<='9'){
    cout <<"IS DIGIT";
}

}
